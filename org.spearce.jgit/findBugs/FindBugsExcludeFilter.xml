<?xml version="1.0" encoding="UTF-8" ?>
<FindBugsFilter>
     <!-- Silence PackFile.mmap calls GC, we need to force it to remove stale
          memory mapped segments if the JVM heap is out of address space.
       -->
     <Match>
       <Class name="org.spearce.jgit.lib.PackFile" />
       <Method name="mmap" />
       <Bug pattern="DM_GC" />
     </Match>
</FindBugsFilter>
